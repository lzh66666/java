		stu2 = new Student2(18001, "李隆基", 18);
		stu3 = new Student2(18003, "孙正贤", 19);
		stu4 = new Student2(18004, "赵匡胤", 20);
		stu5 = new Student2(18005, "孙祥熙", 17);
		stu6 = new Student2(18001, "张剑飞", 20);

		map.put(18001, stu1);
		list.add(stu2);
		list.add(stu3);
		list.add(stu4);

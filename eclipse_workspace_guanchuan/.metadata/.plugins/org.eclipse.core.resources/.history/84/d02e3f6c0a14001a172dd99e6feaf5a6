package nuc.ss.shopping.entiy;

public class User {
	private String id;
	private String type;
	private String password;
	private String name;
	private String sex;
	private String city;
	
	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getName() {
		
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getSex() {
		return sex;
	}

	public void setSex(String sex) {
		this.sex = sex;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public User(String[] strs) {
		this.id = strs[0];
		this.type = strs[1];
		this.password = strs[2];
		this.name = strs[3];
		this.sex = strs[4];
		this.city = strs[5];
	}

	
	public User(String id, String type, String password) {
		super();
		this.id = id;
		this.type = type;
		this.password = password;
	}

	@Override
	public String toString() {
		return id + ":" + type + ":" + password + ":" + name + ":" + sex + ":" 
				+ city;
	}
	
	
	
}
